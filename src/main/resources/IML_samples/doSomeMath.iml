program doSomeMath
global
  input:int64;
  output:int64
do
  debugin input init;
  output init := 1;
  input init := 5;

  switch input
    case 2:
      output := input * 2
    endcase
    case 3:
      output := input * 3
    endcase
    defaultcase:
      if input > 2 then
        output := input - 2
      else
        output := input + 2
      endif
    endcase
  endswitch;
  debugout output   // defaultcase â†’ output = 3
endprogram
